#include <iostream>
#include <vector>
using namespace std;

int main() {
    // keys
    vector<int> k = {10, 231, 345, 81, 33, 87};

    // table size
    int t_s = 10;

    vector<int> hash_table(t_s, -1); //initialize -1 to each index

    int hash_value;
    int key;

     for (int i = 0; i < k.size(); i++) {
        key = k[i];
        hash_value = key % t_s; 

        
        while (hash_table[hash_value] != -1) {
            hash_value = (hash_value + 1) % t_s;
        }

        // place the key
        hash_table[hash_value] = key;
    }
    cout << "Index\tValue" << endl;
    for (int i = 0; i < t_s; i++) {
        cout <<i<< "   \t" << hash_table[i] << endl;
    }
    return 0;
}
